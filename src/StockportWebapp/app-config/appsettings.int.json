  "UseRedisSessions": "true",
  "ExportHost": "http://localhost:80",
  "DemocracyStyleLink": "",
  "ElasticSearchAwsConfiguration:IndexFormat": "iag-webapp-int-{0:yyyy.MM-dd}",
