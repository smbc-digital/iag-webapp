{
  "StylesheetsConfiguration:StyleguideSemantic": "https://s3-eu-west-1.amazonaws.com/smbc-react-assets/qa/styleguide/styles.min.css",
  "StylesheetsConfiguration:StyleguideSemanticV2": "https://s3-eu-west-1.amazonaws.com/smbc-react-assets/qa/styleguide/v2/styles.min.css",
  "UseRedisSessions": "true",
  "ExportHost": "http://localhost:80",
  "DemocracyStyleLink": "",
  "ElasticSearchAwsConfiguration": {
    "IndexFormat": "iag-webapp-qa-{0:yyyy.MM-dd}",
    "MinimumLevel": "Warning",
    "Enabled": true
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore.Localization": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "C:\\Program Files\\Amazon\\ElasticBeanstalk\\logs\\iag-webapp-serilog-{Date}.log",
          "retainedFileCountLimit": 31
        }
      }
    ]
  }
}
