{
  "FeatureManagement:Articles": false,
  "UseAWSSecretManager": true,
  "AWSSecretsManagerConfiguration:EnvironmentGroupPrefix": "iag-prod",
  "GroupManageContactUrl": "/stockport-local-help_hidden",
  "UseRedisSessions": "true",
  "stockportgov": {
    "EmailAlerts": "https://forms.stockport.gov.uk/email-subscription/",
    "EmailAlertsNewSubscriber": "https://forms.stockport.gov.uk/list-subscription/",
    "GoogleAnalytics": "UA-1880211-1",
    "FooterCache": 1440,
    "StylesheetsConfiguration": {
      "StyleguideSemantic": "https://s3-eu-west-1.amazonaws.com/smbc-react-assets/prod/styleguide/styles.min.css",
      "StyleguideSemanticV2": "https://s3-eu-west-1.amazonaws.com/smbc-react-assets/prod/styleguide/v2/styles.min.css",
      "DesignSystemJs": "https://design-system.stockport.gov.uk/prod/1/smbc-frontend.min.js"
    },
    "GroupSubmissionEmail": "website.updates@stockport.gov.uk",
    "GroupArchiveEmail": "website.updates@stockport.gov.uk"
  },
  "healthystockport": {
    "GoogleAnalytics": "UA-80013629-2",
    "FooterCache": 1440
  },
  "ElasticSearchAwsConfiguration": {
    "IndexFormat": "iag-webapp-prod-{0:yyyy.MM-dd}",
    "MinimumLevel": "Warning"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Program Files\\Amazon\\ElasticBeanstalk\\logs\\iag-webapp-serilog-.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-ddTHH:mm:ss} {MachineName} [IAG_WEBAPP] ThreadId:{ThreadId} ProcessId:{ProcessId} {Level:u3} {Message} {NewLine}{Exception}"
        }
      }
    ]
  },
  "GroupArchiveEmailPeriods": [
    {
      "numOfDays": 90,
      "template": "GroupWarningFirstPeriod",
      "subject": "Update your group details"
    },
    {
      "numOfDays": 166,
      "template": "GroupWarningSecondPeriod",
      "subject": "Update your group details"
    },
    {
      "numOfDays": 180,
      "template": "GroupWarningThirdPeriod",
      "subject": "Your group details are now archived"
    }
  ]
}