@using Microsoft.AspNetCore.Mvc.ViewFeatures
@using StockportWebapp.FeatureToggling
@using StockportWebapp.Models
@using StockportWebapp.ProcessedModels
@model StockportWebapp.ProcessedModels.ProcessedOrganisation

@{
    ViewData["Title"] = Model.Title;
    ViewData["og:title"] = Model.Title;
    Layout = "../Shared/_Layout.cshtml";
}

@section Breadcrumbs {
    <partial name="Breadcrumb" model='new List<Crumb>() { new Crumb("Stockport Local", "groups", "groups") }'/>
}

<link rel="stylesheet" media="print" type="text/css" href="/assets/stylesheets/print-organisation-detail.min.css" />

<partial name="Organisations/OrganisationsDetail" model='Model'/>
