@model IEnumerable<SubItem>

<section tabindex="-1" id="content" class="">
    <div class="top-tasks">
            @{
                if (Model != null) 
                {
                    @foreach (var subItem in Model)
                    {
                        <a href="@subItem.NavigationLink" class="top-tasks-block">
                            <div class="top-tasks-block__icon">
                                <span class="@subItem.Icon si-default top-tasks__icon" aria-hidden="true"></span>
                            </div>
                            <div>
                                <h3 class="top-tasks-block__title">@subItem.Title</h3>
                            </div>
                        </a>
                    }
                }
            }
    </div>
</section>