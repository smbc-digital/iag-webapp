@model ContentBlock
@if (Model is not null)
{
    <section class="section section--white">
        <div class="grid-container">
            <div class="grid-100">
                <div class="placeholder placeholder-bg-blue placeholder-small">
                    @foreach(ContentBlock contentBlock in Model.SubItems)
                    {
                        <div class="placeholder placeholder-bg-teal placeholder-small placeholder-inline-margin">
                            <h2>EVENTS CARDS</h2>
                            <p>@contentBlock.Title</p>
                        </div>
                    }
                </div>
            </div>
        </div>
    </section>
}