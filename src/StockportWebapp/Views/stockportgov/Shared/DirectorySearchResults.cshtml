@model DirectoryViewModel

<ul class="navcard-wrapper">
    @if (Model.PaginationInfo.CurrentPage.Equals(1))
    {
        foreach (DirectoryEntryViewModel entry in Model.PinnedEntries)
        {
            <partial name="DirectorySearchResult" model=@entry />
        }
    }

    @foreach (DirectoryEntryViewModel entry in Model.PaginatedEntries)
    {
        <partial name="DirectorySearchResult" model=@entry />
    }
</ul>