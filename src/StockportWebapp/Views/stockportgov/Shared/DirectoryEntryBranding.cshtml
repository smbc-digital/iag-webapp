@model IEnumerable<GroupBranding>
@if (Model.Any())
{
    <div class="directory--border-top directory--no-margin navcard--border-top-10 navcard--theme-teal-lighter-top">
        <partial name="LogoArea" model='Model' view-data='new ViewDataDictionary(ViewData) {{"logoAreaTitle", "In partnership with"}, {"borderTopClass", "navcard-all navcard--theme-grey-accent-all navcard--border-top-0 directory--padding navcard--radius-bottom-left navcard--radius-bottom-right"}}' />
    </div>
}