@model ProcessedEvents
@{
    string eventWrapperCssClasses = (string)ViewData["eventWrapperCssClasses"];
    string borderTop = (string)ViewData["BorderTop"];
}

<div class="event-detail--100 @borderTop">
    <div class="@eventWrapperCssClasses">
        <partial name="LogoArea" model='@Model.EventBranding' view-data='new ViewDataDictionary(ViewData) {{"logoAreaTitleStylingBlank", true}, {"logoAreaArticle", false}, { "logoAreaTitle", @Model.LogoAreaTitle }, {"borderTopClass", "directory--padding"}, {"eventDetailHeading", "event-detail__heading"}}' />
    </div>
</div>