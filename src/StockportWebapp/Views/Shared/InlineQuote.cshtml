@model StockportWebapp.Models.InlineQuote
<div class="inline-quote-container">
    <img src="@Model.Image" alt="@Model.ImageAltText" />
    <h4>@Model.Author</h4>
    <div class="quote-body">
        <span class="green open fa fa-2x fa-quote-left" aria-hidden="true"></span>
        <p class="lead-paragraph">@Model.Quote</p>
        <span class="green fa fa-2x fa-quote-right" aria-hidden="true"></span>
    </div>
</div>