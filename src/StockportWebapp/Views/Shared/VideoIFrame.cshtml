@model VideoViewModel;

@if (!string.IsNullOrEmpty(Model.VideoToken) && !string.IsNullOrEmpty(Model.PhotoId))
{
    string url = $"https://video.stockport.gov.uk/v.ihtml/player.html?token={Model.VideoToken}&source=embed&photo%5fid={Model.PhotoId}";
    <iframe title="@Model.Title" src="@url" style="width:100%; height:100%; position:absolute; top:0; left:0;" frameborder="0" scrolling="no" allowfullscreen allow="autoplay; fullscreen">
    </iframe>  
}