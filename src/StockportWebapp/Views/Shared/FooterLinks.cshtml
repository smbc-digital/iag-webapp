@using StockportWebapp.Config
@model IEnumerable<StockportWebapp.Models.SubItem>
@inject BusinessId BusinessId

@foreach (var link in Model)
{
<li class="grid-50 tablet-grid-50 mobile-grid-100">
    <a href="@link.NavigationLink">@link.Title</a>
</li>
}
@if (BusinessId.ToString() == "healthystockport")
{
    <li class="grid-50 tablet-grid-50 mobile-grid-100">
        <a class="typeform-share link" data-mode="1" target="_blank" href="https://stockportcouncil.typeform.com/to/X5KuXC">Feedback</a>
    </li>
}
else
{
    <li class="grid-50 tablet-grid-50 mobile-grid-100">
        <a class="typeform-share link" data-mode="1" target="_blank" href="https://stockportcouncil.typeform.com/to/dXTax7">Feedback</a>
    </li>
}

