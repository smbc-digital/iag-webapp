    <PackageReference Include="Serilog.Settings.Configuration" Version="2.6.1" />
    <PackageReference Include="Serilog.Sinks.Console" Version="3.1.1" />
    <PackageReference Include="Serilog.Sinks.ElasticSearch" Version="6.5.0" />
    <PackageReference Include="Serilog.Sinks.RollingFile" Version="3.3.0" />
    <PackageReference Include="WebMarkupMin.AspNetCore1" Version="2.4.4" />
    <PackageReference Include="YamlDotNet" Version="5.0.1" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="2.1.1" />
    <PackageReference Include="Humanizer.Core" Version="2.4.2" />
    <PackageReference Include="Quartz" Version="3.0.6" />
    <PackageReference Include="WilderMinds.RssSyndication" Version="1.2.0" />
    <PackageReference Include="AngleSharp" Version="0.9.9.2" />
